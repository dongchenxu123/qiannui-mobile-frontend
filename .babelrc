{
    "presets": ["es2015", "stage-0", "react"],
    "plugins": [
        "add-module-exports",
        "transform-react-display-name",
        "transform-class-properties",
        "transform-decorators-legacy",
        ["transform-react-jsx", { "pragma": "createElement" }],
        ["transform-imports", {
                "nuke": {
                    "transform": "nuke/lib/${member}",
                    "preventFullImport": true
                },
        }]
    ]
}